{
    "extends": [
        "airbnb-base"
    ],
    "env": {
        "browser": false,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "jsx-quotes": [
            "error",
            "prefer-single"
        ],
        "no-param-reassign": 0,
        "global-require": 0,
        "max-len": 0
    }
}