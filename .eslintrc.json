{
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "browser": false,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "jsx-quotes": [
      "error",
      "prefer-single"
    ],
    "no-param-reassign": 0,
    "global-require": 0,
    "max-len": 0,
    "linebreak-style": 0,
    "no-nested-ternary": 0,
    "consistent-return": 0,
    "no-continue": 0,
    "no-plusplus": 0,
    "import/no-dynamic-require": 0
  }
}